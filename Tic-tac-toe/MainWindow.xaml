<Window x:Class="Tic_tac_toe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tic_tac_toe"
        mc:Ignorable="d"
        xmlns:renderer="clr-namespace:Tic_tac_toe.Render"
        Title="MainWindow" Height="300" Width="300" MouseLeftButtonDown="Window_MouseDown" SizeChanged="Window_SizeChanged" Loaded="Window_Loaded">
    <Grid x:Name="grid">
        <renderer:Display x:Name="display"></renderer:Display>
    </Grid>
</Window>
